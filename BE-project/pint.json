{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "no_superfluous_phpdoc_tags": false,
        "declare_strict_types": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "concat_space": {
            "spacing": "one"
        }
    }
}